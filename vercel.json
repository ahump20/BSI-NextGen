{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "version": 2,
  "buildCommand": "pnpm install && pnpm --filter @bsi/web build",
  "outputDirectory": "packages/web/.next",
  "installCommand": "pnpm install",
  "framework": "nextjs",
  "devCommand": "pnpm --filter @bsi/web dev",
  "ignoreCommand": "git diff --quiet HEAD^ HEAD ./",
  "regions": ["iad1"],
  "github": {
    "enabled": true,
    "autoAlias": true,
    "silent": false,
    "autoJobCancellation": true
  },
  "build": {
    "env": {
      "CI": ""
    }
  }
}
